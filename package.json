{
  "name": "email-cadence-coding-exam",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:web\" \"npm run dev:api\" \"npm run dev:worker\"",
    "dev:web": "npm --workspace=web run dev",
    "dev:api": "npm --workspace=api run start:dev",
    "dev:worker": "npm --workspace=worker run dev"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}
